<h1>webpages</h1>

<table class="table mt-3">
  <thead class="thead-dark">
    <tr>
      <th>URL</th>
      <th>Element</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @webpages.each do |webpage| %>
      <tr>
        <td><a href="<%= webpage.url %>" target="_blank" rel="nofollow"><%= webpage.url %></a></td>
        <td><%= webpage.element %></td>
        <td><%= link_to 'Edit', edit_webpage_path(webpage), class: 'btn btn-secondary' %></td>
        <td><%= link_to 'Destroy', webpage, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New webpage', new_webpage_path, class: 'btn btn-primary' %>
