Host server
	IdentityFile ~/.ssh/lightsail.pem
	User admin
	Hostname server.bartol.dev

Host rpi
	Port 2002
	Hostname home.bartol.dev

Host rpi2
	User pi
	Port 2222
	Hostname home2.bartol.dev

Host pc
	ProxyJump rpi
	Hostname 192.168.100.28

Host laptop2
	ProxyJump rpi2
	User danica
	Hostname 192.168.2.101
#	Hostname 192.168.2.102
